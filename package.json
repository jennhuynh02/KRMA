{
  "name": "treasure-box",
  "version": "1.0.0",
  "description": "Treasure Box is a web application where users can place media in a treasure box in exchange for a key.  Keys can be used to fetch an item out of one of the treasure boxes.",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jennhuynh02/treasure-box.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jennhuynh02/treasure-box/issues"
  },
  "homepage": "https://github.com/jennhuynh02/treasure-box#readme",
  "dependencies": {
    "aws-sdk": "^2.674.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3"
  }
}
